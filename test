-- Fake Module Scripts:

local TweenService = game:GetService("TweenService")

GUI.StartPauseButton_0.MouseEnter:Connect(function()
	TweenService:Create(GUI.UIScale_0, TweenInfo.new(0.2, Enum.EasingStyle.Circular, Enum.EasingDirection.Out), {Scale = 1.03}):Play()
	TweenService:Create(GUI.UIGradient_01, TweenInfo.new(0.4, Enum.EasingStyle.Back, Enum.EasingDirection.Out), {Offset = Vector2.new(0, 1)}):Play()
end)

GUI.StartPauseButton_0.MouseLeave:Connect(function()
	TweenService:Create(GUI.UIScale_0, TweenInfo.new(0.3, Enum.EasingStyle.Circular, Enum.EasingDirection.Out), {Scale = 1}):Play()
	TweenService:Create(GUI.UIGradient_01, TweenInfo.new(0.4, Enum.EasingStyle.Back, Enum.EasingDirection.Out), {Offset = Vector2.new(0, 0)}):Play()
end)

local Table = {
	Seeds = {
		Carrot = {
			Image = "rbxassetid://111808451137805",
			InStock = false,
			Order = 1,
			AutoBuy = false
		}
	}
}

-- Seed Stock Checker:
coroutine.wrap(function()
	while wait() do
		for ItemName, ItemInfo in Table.Seeds do
			if game:GetService("Players").LocalPlayer.PlayerGui.Seed_Shop.Frame.ScrollingFrame[ItemName].Main_Frame.Stock_Text.Text == "X0 Stock" then
				Table.Seeds[ItemName].InStock = false
			else
				Table.Seeds[ItemName].InStock = true
			end
		end
	end
end)()

for Category, Items in Table do
	for ItemName, ItemInfo in Items do
		local Clone = GUI.Template_0:Clone()

		Clone.Name = ItemName
		Clone.Parent = GUI.ScrollingFrame_0
		Clone.LayoutOrder = ItemInfo.Order+1
		Clone.Visible = true
		Clone.Checkbox.MouseButton1Down:Connect(function()
			if ItemInfo.AutoBuy == true then
				ItemInfo.AutoBuy = false
				Clone.Checkbox.ImageTransparency = 1
			else
				ItemInfo.AutoBuy = true
				Clone.Checkbox.ImageTransparency = 0
			end
		end)
	end
end

local IsScriptRunning = false

coroutine.wrap(function()
	while wait() do
		if IsScriptRunning == true then
			for Category, Items in Table do
				if Category == "Seeds" then
					for SeedName, SeedInfo in Items do
						if SeedInfo.AutoBuy == true and SeedInfo.InStock == true then
							game:GetService("ReplicatedStorage"):WaitForChild("GameEvents"):WaitForChild("BuySeedStock"):FireServer(SeedName)
						end
					end
				end
			end
		end
	end
end)()

local function Notify(Text)
	GUI.Notice_0.Text = Text
end

GUI.StartPauseButton_0.MouseButton1Down:Connect(function()
	if IsScriptRunning == false then
		IsScriptRunning = true
		Notify("Script running!")
	else
		IsScriptRunning = false
		Notify("Script stopped...")
	end
end)
